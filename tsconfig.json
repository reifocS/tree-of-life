  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "prisma/script.mjs", "prisma/userss.mjs", "pages/api/treeVersion/index.js", "pages/api/treeVersion/[treeVersionId]/branch/index.js", "pages/api/treeMaster/[treeMasterId]/treeVersion/index.js", "pages/api/leaf/index.js", "pages/api/branch/index.js", "pages/api/users.js", "pages/api/user/[userId]/treeMaster/index.js"],
  "exclude": ["node_modules"]
}
